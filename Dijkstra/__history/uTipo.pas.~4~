unit uTipo;

interface

uses
  Generics.Collections;

type

  TVertice = record
    FSigla: string;
    FEstado: string;
  end;

  TAresta = record
    FVertice1: TVertice;
    FVertice2: TVertice;
    FValor: Integer;
  end;

  TCaminho = record
    Arestas: TDictionary<integer,TAresta>;
    Valor: integer;
  end;

implementation

end.
